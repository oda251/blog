---
import Layout from './Layout.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
interface Props {
	title: string;
}
const { title } = Astro.props;
---

<Layout title={title}>
	<header class="h-[3rem] bg-white mb">
		<Header />
	<header />
	<slot />
	<Footer />
</Layout>