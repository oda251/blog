---
import BaseLayout from '../../layouts/BaseLayout.astro'
import { TweetApp } from '../../components/Tweet/TweetApp'
import TweetRepository from '../../repository/tweetRepository'
const title = 'blog'

const repository = new TweetRepository()
const initTweets = await repository.fetchTweetByLastId(null)
---

<BaseLayout title={title}>
  <TweetApp client:only="react" tweets={initTweets} />
</BaseLayout>
